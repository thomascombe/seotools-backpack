{
    "name": "thomascombe/seotools-backpack",
    "description": "SEO Tools for Laravel Backpack",
    "keywords": [
        "seotoolsbackpack",
        "seotools-backpack"
    ],
    "homepage": "https://github.com/thomascombe/seotools-backpack",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Combe",
            "email": "thomascombe42@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/support": "5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Thomascombe\\SeotoolsBackpack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thomascombe\\SeotoolsBackpack\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thomascombe\\SeotoolsBackpack\\SeotoolsBackpackServiceProvider"
            ]
        }
    }
}
